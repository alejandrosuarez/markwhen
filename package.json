{
  "name": "renderer",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "link:parser": "yalc add @markwhen/parser && yalc link @markwhen/parser && npm install",
    "unlink:parser": "yalc remove @markwhen/parser && npm install"
  },
  "dependencies": {
    "@markwhen/parser": "^0.2.3",
    "@squadette/hammerjs": "^2.1.0-pre3",
    "@vueuse/core": "^9.3.1",
    "@vueuse/integrations": "^9.3.1",
    "chrono-node": "^2.4.1",
    "lru-cache": "^7.14.0",
    "lunr": "^2.3.9",
    "luxon": "^3.1.0",
    "pinia": "^2.0.23",
    "throttle-debounce": "^5.0.0",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jsdom": "^16.2.14",
    "@types/lunr": "^2.3.4",
    "@types/luxon": "^3.0.2",
    "@types/node": "^16.11.45",
    "@types/throttle-debounce": "^5.0.0",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.0",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/head": "^0.9.8",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^9.0.0",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vitest": "^0.24.3",
    "vue-tsc": "^1.0.9"
  }
}
